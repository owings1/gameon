{
  "name": "gameon",
  "version": "1.1.2",
  "private": true,
  "description": "backgammon",
  "main": "index.js",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc --reporter=text-summary mocha test/test.js && nyc report --reporter=lcov",
    "version": "oclif-dev readme && git add README.md",
    "install": "node scripts/install.js",
    "mac": "oclif-dev pack:macos",
    "dist": "oclif-dev pack",
    "dist_and_publish": "oclif-dev pack && oclif-dev publish"
  },
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-update": "^1.3.10",
    "aws-sdk": "^2.768.0",
    "better-logging": "^3.5.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "express-requests-logger": "^3.0.2",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.0.4",
    "merge": "^1.2.1",
    "node-fetch": "^2.6.1",
    "strip-ansi": "^6.0.0",
    "uuid": "^7.0.3",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "mocha": "^6.1.4",
    "nyc": "^15.0.0",
    "tmp": "^0.1.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/scripts"
  ],
  "repository": "https://git.int.dougowings.net/owings/gameon.git",
  "bin": {
    "gameon": "./bin/run"
  },
  "oclif": {
    "commands": "./src/commands",
    "autocomplete": {
      "bin": "gameon"
    },
    "bin": "gameon",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-autocomplete"
    ],
    "hooks": {
      "init": "./src/hooks/init",
      "update": "./src/hooks/update"
    },
    "topics": {
      "robot": {
        "description": "Mess around with Robots"
      }
    },
    "update": {
      "s3": {
        "bucket": "gameon"
      }
    },
    "macos": {
      "identifier": "net.dougowings.gameon"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Doug Owings <doug@dougowings.net>"
}
